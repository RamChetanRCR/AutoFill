{
  "manifest_version": 3,
  "name": "Applications Autofill",
  "version": "1.0",
  "description": "Autofill job application forms",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
